{
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "editor.fontFamily": "'Fira Code Medium', 'monospace', monospace, 'Fira Code Medium'",
  "go.autocompleteUnimportedPackages": true,
  "diffEditor.renderSideBySide": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "markdownlint.config": {
    "MD022": false,
    "MD024": false,
    "MD025": false
  },
  "[go]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },
  "go.lintTool": "golangci-lint",
  "go.docsTool": "guru",
  "go.useLanguageServer": true,
  "yaml.customTags": ["!vault"],
  "yaml.format.enable": true,
  "files.associations": {
    // tasks
    "**/tasks/*.yaml": "ansible",
    "**/tasks/*.yml": "ansible",
    // handlers
    "**/handlers/*.yaml": "ansible",
    "**/handlers/*.yml": "ansible",
    // plays
    "**/playbook/*.yaml": "ansible",
    "**/playbook/*.yml": "ansible",
    "**/play/*.yml": "ansible",
    "**/play/*.yaml": "ansible",
    "**/plays/*.yml": "ansible",
    "**/plays/*.yaml": "ansible",
    "playbook.yml": "ansible",
    "playbook.yaml": "ansible",
    "*plays.yml": "ansible",
    // inventory
    "**/inventory/*.yaml": "ansible",
    "**/inventory/*.yml": "ansible",
    "inventory.yml": "ansible",
    "inventory.yaml": "ansible",
    // galaxy
    "galaxy.yml": "ansible",
    "galaxy.yaml": "ansible"
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  //"editor.multiCursorModifier": "altCmd",
  "editor.multiCursorModifier": "ctrlCmd",
  "files.trimFinalNewlines": true,
  "editor.renderWhitespace": "all",
  "sync.autoDownload": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "[Dd]ockerfile*": "dockerfile",
    "[Dd]ocker-compose-*.yml": "docker-compose"
  },
  "sync.autoUpload": true,
  "sync.quietSync": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "terraform.telemetry.enabled": false,
  "git.alwaysShowStagedChangesResourceGroup": true,
  "terraform.languageServer": {
    "external": true,
    "pathToBinary": "/usr/local/bin",
    "args": [],
    "maxNumberOfProblems": 100,
    "trace.server": "off"
  },
  "go.formatTool": "gofmt",
  //"rust-client.trace.server": "verbose",
  //"rust-client.updateOnStartup": true,
  //"rust-analyzer.path": "~/.config/Code/User/globalStorage/matklad.rust-analyzer",
  //"rust-analyzer.trace.extension": true,
  //"rust-analyzer.trace.server": "verbose",
  "gitlens.views.repositories.showBranchComparison": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "lldb.launch.cwd": "${fileDirname}",
  "lldb.verboseLogging": true,
  "projectManager.git.baseFolders": [
    "$home/git",
    "$home/git_1win",
    "$home/github"
  ],
  "projectManager.git.maxDepthRecursion": 8,
  "git.confirmSync": false,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "remote.containers.dockerPath": "podman",
  "window.menuBarVisibility": "visible",
  "files.exclude": {
    "**/.git": false
  },
  "editor.renderControlCharacters": true,
  "rust-client.trace.server": "verbose",
  "debug.allowBreakpointsEverywhere": true,
  "[dockerfile]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "gitlens.advanced.blame.customArguments": [],
  "yaml.format.printWidth": 120,
  "editor.formatOnType": true,
  "diffEditor.codeLens": true,
  "terminal.explorerKind": "external",
  "python.showStartPage": false,
  "deepcode.advancedMode": true,
  "deepcode.yesCrashReport": false,
  "deepcode.yesTelemetry": false,
  "deepcode.yesWelcomeNotification": false,
  "deepcode.uploadApproved": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#3f0e1e"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "json.maxItemsComputed": 50000,
  "go.toolsManagement.autoUpdate": true,
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.useCodeSnippetsOnFunctionSuggestWithoutType": true,
  "telemetry.telemetryLevel": "off",
  "redhat.telemetry.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "[rust]": {
    "editor.tabSize": 2
  },
  "settingsSync.ignoredExtensions": [],
  "editor.rulers": [80, 120],
  "gitlens.views.tags.files.layout": "list",
  "projectManager.removeCurrentProjectFromList": false,
  "projectManager.showParentFolderInfoOnDuplicates": true,
  "projectManager.filterOnFullPath": true,
  "extensions.ignoreRecommendations": true,
  "github.copilot.editor.enableAutoCompletions": true
}
